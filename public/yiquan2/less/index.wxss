}
.submit-infor .submit-content .form .form-second-item:before,
.submit-infor .submit-content .form .form-second-item:after {
  content: " ";
  display: table;
}
.submit-infor .submit-content .form .form-second-item:after {
  clear: both;
}
.submit-infor .submit-content .form .form-second-item .form-input {
  float: left;
  width: 45%;
}
.submit-infor .submit-content .form .form-second-item .form-input:last-child {
  float: right;
}
.submit-infor .submit-content .form .form-three-item:before,
.submit-infor .submit-content .form .form-three-item:after {
  content: " ";
  display: table;
}
.submit-infor .submit-content .form .form-three-item:after {
  clear: both;
}
.submit-infor .submit-content .form .form-three-item .form-input {
  float: left;
  width: 60%;
