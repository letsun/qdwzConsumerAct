  width: 40%;
  height: 60px;
  line-height: 60px;
  text-align: center;
  color: #c23f44;
  font-size: 26px;
  border-radius: 0 6px 6px 0;
  background: #fbe4b8;
}
.submit-infor .submit-content .form .form-other-item {
  margin-top: 6px;
}
.submit-infor .submit-content .close-infor {
  position: absolute;
  top: 2%;
  right: 3%;
  z-index: 10;
  width: 10%;
}
.loading {
  position: fixed;
  z-index: 9999;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.loading .loadCon {
  position: absolute;
  top: 40%;
  left: 50%;
  width: 150px;
  height: 150px;
  margin-top: -75px;
  margin-left: -75px;
}
.loading .loadCon img {
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  width: 100%;
  -webkit-animation: load 4s linear infinite;
}
.loading .loadCon #percent {
  font-size: 24px;
  line-height: 150px;
  position: absolute;
  top: 0;
  left: 0;
  width: 150px;
  height: 150px;
  text-align: center;
}
@-webkit-keyframes scale {
  0% {
    -webkit-transform: scale(1);
  }
  100% {
    -webkit-transform: scale(0.7);
  }
}
