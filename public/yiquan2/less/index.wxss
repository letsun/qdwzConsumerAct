}
.submit-infor .submit-content .form .form-item .form-text {
  padding: 0 10px;
  font-size: 24px;
  color: #ecc8a3;
}
.submit-infor .submit-content .form .form-item .form-submit {
  width: 371px;
  height: 70px;
  margin: 0 auto;
  font-size: 28px;
  text-align: center;
  line-height: 70px;
  color: #a52c30;
  background: url('https://qdwzvue-1254182596.cos.ap-guangzhou.myqcloud.com/qdwzAct/luoboke/1_8.png') no-repeat;
  background-size: 100% 100%;
