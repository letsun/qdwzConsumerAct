<% var couponName = {
'0': '代金券',
'1': '折扣券',
'2': '兑换券',
'3': '满减券',
}; %>

<div class="result-bg"></div>
<div class="result-container">

	<div class="result">
		<% if (result.code === 200) { %><div class="tip-status <% if (result.data.type === 1) { %>top<% } %>">恭喜您获得</div><% } else { %><div class="tip-status">很遗憾</div><% } %>
		
		<% if (result.code === 200 && result.data.type == 1) { %>
			<% var couponGrants = result.data.couponGrants[0]; %>
			<% if (couponGrants.couponType == 0 || couponGrants.couponType == 1 || couponGrants.couponType == 2 || couponGrants.couponType == 3) {%>
			<div class="block" style="display: block;">
				<div class="coupon-wrapper">
					<div class="coupon-container">
						<div class="coupon-left">
							<div class="coupon-top">
								<%= couponGrants.couponName %>
							</div>
							<div class="coupon-bottom">
								编号：<%= couponGrants.couponCode %>
							</div>
						</div>
						<div class="coupon-right"><%= couponName[couponGrants.couponType] %></div>
					</div>
				</div>
				<div class="coupon-tip">请到有赞商城核销优惠券</div>
				<a href="https://h5.youzan.com/v2/showcase/homepage?alias=v43ns3tOmH" class="use-btn">
					<img src="https://qdwzvue-1254182596.cos.ap-guangzhou.myqcloud.com/qdwzAct/notbean/images/1/2_5.png" />
				</a>
			</div>
			<% } %>
		<% } else if (result.code === 200 || result.code === 201) { %>

			<% if (result.code === 200) { %>
			<div class="block" style="display: block;">
				<div class="redPackage-wrapper">
					<div class="redPackage-price"><span>￥</span><%= result.data.redPack.prizeAmount %></div>
					<div class="redPackage-tip">现金红包</div>
				</div>
			</div>

			<% } else { %>

			<div class="block" style="display: block;">
				<div class="noAward-wrapper">
					<div class="noAward-logo">
						<img src="https://qdwzvue-1254182596.cos.ap-guangzhou.myqcloud.com/qdwzAct/notbean/images/1/4_1.png" />
					</div>
					<div class="noAward-tip"><%= result.msg %></div>
				</div>
			</div>

			<% } %>

		<% } %>

		<div class="qrcode">
			<img src="https://qdwzvue-1254182596.cos.ap-guangzhou.myqcloud.com/qdwzAct/notbean/images/1/2_6.png" />
			<div class="qrcode-tip">长按关注公众号</div>
		</div>
	</div>

	<img class="close-result" src="https://qdwzvue-1254182596.cos.ap-guangzhou.myqcloud.com/qdwzAct/notbean/images/1/1_200.png" alt="">
</div>
<img src="https://qdwzvue-1254182596.cos.ap-guangzhou.myqcloud.com/qdwzAct/notbean/images/1/2_4.png" class="abs img6" />